      "this"
    ],

    "variables": [
      "$wpdb",
      "$post",
      "$user_ID",
      "$current_user",

      "forceStandaloneForTests"
    ],

    "constants": [
      "ABSPATH",
      "PLUGIN_VERSION",
      "WPINC"
    ]

  }
}
