{
  "whitelist": {

    "dirs_to_obfuscate": [
      "includes/license",
      "includes/core"
    ],
    "exclude_files": [
      "includes/license/LicenseMessages.php",
      "includes/license/LicenseConstants.php",
      "includes/license/LicensePlans.php"
    ],
    "exclude_patterns":[
      "autoload.php",
      "helpers.php"
    ],
    "functions": [
      "add_action",
      "add_filter",
      "do_action",
      "apply_filters",
      "register_activation_hook",
      "register_deactivation_hook",
      "__",
      "_e",
